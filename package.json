{
  "name": "store-redis",
  "version": "0.1.1",
  "description": "A simple redis store for backbone-orm's query caching",
  "main": "./lib/redis_store",
  "repository": {
    "type": "git",
    "url": "https://github.com/vidigami/store-redis.git"
  },
  "scripts": {
    "build": "coffee -o lib -c src",
    "watch": "coffee -o lib -w src",
    "clean": "rm -rf lib/*",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "postinstall": "npm run build"
  },
  "keywords": [
    "redis",
    "store",
    "backbone-orm"
  ],
  "dependencies": {
    "redis": "0.9.x",
    "underscore": "*"
  },
  "devDependencies": {
    "coffee-script": "*"
  },
  "license": "MIT"
}
